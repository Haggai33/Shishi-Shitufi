{
  "rules": {
    "events": {
      ".read": true,
      ".write": "auth != null && (auth.token.email == 'chagai33@gmail.com' || root.child('admins').child(auth.uid).val() == true)"
    },
    "menuItems": {
      ".read": true,
      ".write": true
    },
    "assignments": {
      ".read": true,
      ".write": true
    },
    "admins": {
